{
	"version": 1,
	"formatters": {
		"standard": {
			"format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
			"datefmt": "%Y-%m-%d %H:%M:%S"
		}
	},
	"handlers": {
		"console": {
			"class": "logging.StreamHandler",
			"formatter": "standard",
			"stream": "ext://sys.stdout",
			"level": "INFO"
		}
	},
	"loggers": {
		"root": {
			"handlers": ["console"],
			"propagate": 1,
			"level": "INFO"
		}
	}
}
